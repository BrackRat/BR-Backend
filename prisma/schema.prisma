datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "cargo prisma"
  output   = "../src/db.rs"
}

model User {
  id                     Int    @id @unique @default(autoincrement())
  registerUnixTimestamp  BigInt
  lastloginUnixTimestamp BigInt
  name                   String @unique
  password               String
}
